{
    "name": "@fdmg/design-system",
    "version": "0.4.27",
    "description": "FD Design System",
    "types": "main.d.ts",
    "main": "main.js",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:FDMediagroep/fd-design-system.git"
    },
    "scripts": {
        "build": "npx next build",
        "build:ci": "npm run build && npm run build:npm",
        "build:npm": "npm run webpack:prod && npm run copy:dts",
        "build:sls": "npx cross-env SERVERLESS=true next build",
        "copy:dts": "npx cpx -v \"./dist.d.ts/src/**/*.d.ts\" \"./dist\"",
        "dev": "next dev",
        "lint": "eslint \"src/**\"",
        "json-to-scss": "json-to-scss public/assets/design-tokens/* src/design-tokens",
        "prettier": "prettier -c **/*.ts?",
        "start": "npx next build && npx next start",
        "test": "jest --passWithNoTests",
        "test:updateSnapshots": "jest -u --passWithNoTests",
        "typedoc": "npx typedoc",
        "webpack:prod": "npx cross-env NODE_ENV=production webpack --mode=production",
        "webpack-bundle-analyzer:npm": "npx cross-env WEBPACK_BUNDLE_ANALYZER=true npm run webpack:prod",
        "webpack-bundle-analyzer:next": "npx cross-env WEBPACK_BUNDLE_ANALYZER=true next build"
    },
    "author": "Willem Liu",
    "license": "ISC",
    "devDependencies": {
        "@fdmg/css-grid": "^2.0.6",
        "@testing-library/jest-dom": "^5.11.5",
        "@testing-library/react": "^11.1.1",
        "@testing-library/user-event": "^12.2.0",
        "@types/fetch-jsonp": "^1.0.0",
        "@types/highlight.js": "^9.12.4",
        "@types/jest": "^26.0.15",
        "@types/js-cookie": "^2.2.6",
        "@types/micro-cors": "^0.1.1",
        "@types/node": "^14.14.6",
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/xmldom": "^0.1.29",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@typescript-eslint/typescript-estree": "^4.6.1",
        "autoprefixer": "^10.0.1",
        "copy-webpack-plugin": "^6.3.0",
        "core-js": "^3.6.5",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "date-input-polyfill": "^2.14.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "fetch-jsonp": "^1.1.3",
        "highlight.js": "^10.3.2",
        "jest": "^25.5.4",
        "jest-css-modules": "^2.1.0",
        "json-to-scss": "^1.6.2",
        "micro-cors": "^0.1.1",
        "mini-css-extract-plugin": "^1.2.1",
        "next": "^10.0.1",
        "next-offline": "^5.0.3",
        "nookies": "^2.5.0",
        "path-to-regexp": "^6.2.0",
        "postcss-flexbugs-fixes": "^5.0.0",
        "postcss-loader": "^4.0.4",
        "postcss-normalize": "^9.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.1.2",
        "pretty": "^2.0.0",
        "raw-loader": "^4.0.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-element-to-jsx-string": "^14.3.2",
        "react-lowlight": "^2.0.0",
        "react-markdown": "^5.0.2",
        "resize-observer-polyfill": "^1.5.1",
        "sass": "^1.29.0",
        "sass-loader": "^10.0.5",
        "style-loader": "^2.0.0",
        "svgo": "^1.3.2",
        "svgo-loader": "^2.2.1",
        "ts-jest": "^25.5.1",
        "ts-loader": "^8.0.10",
        "ts-node": "^9.0.0",
        "typedoc": "^0.19.2",
        "typescript": "^3.9.7",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^4.2.0",
        "xmldom": "^0.4.0"
    },
    "dependencies": {
        "@webcomponents/webcomponentsjs": "^2.5.0",
        "js-cookie": "^2.2.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 edge version",
            "ie 11"
        ]
    }
}
