{
    "version": 2,
    "regions": ["dub1"],
    "headers": [
        {
            "source": "/_next/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, max-stale, s-maxage=5, stale-while-revalidate=30"
                }
            ]
        },
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, max-stale, s-maxage=86400, stale-while-revalidate"
                }
            ]
        },
        {
            "source": "/favicon.ico",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, max-stale, s-maxage=86400, stale-while-revalidate"
                }
            ]
        }
    ]
}
